<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="crossBrowserTesting" parallel="methods">
    <listeners>
        <listener class-name="c11Practice.listeners.MethodListener"/>
    </listeners>
    <test name="chrome">
        <parameter name="browserName" value="chrome"/>
        <classes>
            <class name="c11Practice.paralelTesting.crossBrowser.CrossBrowserTesting"/>
        </classes>
    </test>
    <test name="firefox">
        <parameter name="browserName" value="firefox"/>
        <classes>
            <class name="c11Practice.paralelTesting.crossBrowser.CrossBrowserTesting"/>
        </classes>
    </test>
</suite>