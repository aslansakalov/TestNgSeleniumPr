<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="paralelExecCrhomeAndFireFox" parallel="tests" thread-count="2">
    <test name="chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="c11Practice.paralelTesting.test.ParalelExecOnTest"/>
